{
  "name": "cray-generators",
  "version": "0.1.0",
  "description": "Various generators for different common project types at Cray",
  "repository": "https://stash.us.cray.com/projects/CLOUD/repos/cray-generators/",
  "license": "Apache-2.0",
  "homepage": "https://stash.us.cray.com/projects/CLOUD/repos/cray-generators/browse/README.md",
  "private": true,
  "author": {
    "name": "Cray",
    "email": "casm-cloud@cray.com"
  },
  "contributors": [
    {
      "name": "Patrick Force",
      "email": "pforce@cray.com"
    }
  ],
  "scripts": {
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "devDependencies": {},
  "engines": {
    "node": ">= 10",
    "npm": ">= 6.4"
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "fs-extra": "^7.0.1",
    "yeoman-generator": "^3.1.1",
    "yosay": "^2.0.2"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  }
}
